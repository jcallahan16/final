

<% all_athletes = Athlete.read %>

<% all_athletes.each do |athlete| %>

<% athlete_fname = athlete["first_name"] %>
<% athlete_lname = athlete["last_name"] %>
<% athlete_name = athlete_fname + athlete_lname %>

<li> <%= link_to (athlete_fname), "/athlete/#{athlete["id"]}" %> &nbsp; <%= link_to (athlete_lname), "/athlete/#{athlete["id"]}" %> </li>
<% end %>